pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload')
        {
            steps
            {
                git 'https://github.com/atif10iqbal/JenkinsProject.git'
            }
        }
        stage('ContinuousBuild')
        {
            steps
            {
                echo 'mvn package'
            }
        }
    }
}