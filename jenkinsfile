pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload')
        {
            steps
            {
                git 'https://github.com/atif10iqbal/JenkinsProject.git'
            }
        }
        stage('ContinuousBuild')
        {
            steps
            {
                script {
                try {
            withCredentials([sshUserPrivateKey(credentialsId: 'jen', keyFileVariable: 'j', passphraseVariable: 'p', usernameVariable: 'u')]) {
    // some block
}
                sh 'la -la'
                }
                catch (Exception e1){
                    echo 'error'
                }
            }
            }
        }
    }
}